{
  "name": "webpack-plugin-downgrade",
  "version": "0.1.0",
  "description": "Weex downgrade plugin",
  "main": "index.js",
  "scripts": {
    "build": "npm run build:downgrade && npm run build:plugin",
    "build:downgrade": "rollup -c build/rollup.downgrade.config.js",
    "prebuild:plugin": "rollup -c build/rollup.downgrade.plugin.config.js",
    "build:plugin": "rollup -c build/rollup.plugin.config.js",
    "dev:downgrade": "rollup -w -c build/rollup.downgrade.config.js",
    "dev:plugin": "rollup -w -c build/rollup.plugin.config.js",
    "test": "webpack --config examples/webpack.config.js"
  },
  "keywords": [
    "webpack",
    "plugin",
    "weex",
    "downgrade"
  ],
  "author": "Hanks <zhanghan.me@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@weex-project/downgrade": "^1.0.1",
    "json-format": "^1.0.1",
    "webpack": "^2.6.1",
    "webpack-sources": "^1.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.0",
    "buble": "^0.15.2",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-uglify": "^2.0.1",
    "rollup-watch": "^3.2.2",
    "weex-loader": "^0.4.5"
  }
}
